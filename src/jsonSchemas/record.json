{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "account_id": {
        "type": "string"
      },
      "batch_id": {
        "type": "integer"
      },
      "counter_account_id": {
        "type": "string"
      },
      "counter_account_name": {
        "type": "string"
      },
      "cumulative_balance": {
        "type": "number"
      },
      "cumulative_balance_by_sector": {
        "type": "number"
      },
      "cumulative_balance_by_sort_key": {
        "type": "number"
      },
      "cumulative_balance_without_opening_balance": {
        "type": "number"
      },
      "cumulative_balance_of_open_sum_in_record": {
        "type": "number"
      },
      "debit_or_credit": {
        "type": "string",
        "enum": ["חובה", "זכות"]
      },
      "debit_or_credit_number": {
        "type": "integer",
        "enum": [-1, 1]
      },
      "estimated_sum": {
        "type": "number"
      },
      "foreign_currency_credit": {
        "type": ["number", "null"]
      },
      "foreign_currency_cumulative_balance": {
        "type": "number"
      },
      "foreign_currency_cumulative_balance_without_opening_balance": {
        "type": "number"
      },
      "foreign_currency_debit": {
        "type": ["number", "null"]
      },
      "foreign_currency_sum": {
        "type": "number"
      },
      "foreign_currency_sum_closed_in_record": {
        "type": "number"
      },
      "foreign_currency_sun_open_in_record": {
        "type": "number"
      },
      "id": {
        "type": "integer"
      },
      "match_number_card_analysis": {
        "type": ["integer", "null"]
      },
      "shekel_credit": {
        "type": ["number", "null"]
      },
      "shekel_debit": {
        "type": ["number", "null"]
      },
      "shekel_sum": {
        "type": "number"
      },
      "shekel_sum_closed_in_record": {
        "type": "number"
      },
      "shekel_sum_open_in_record": {
        "type": "number"
      },
      "transaction_id": {
        "type": "integer"
      }
    },
    "required": ["id", "account_id", "batch_id", "transaction_id"]
  }
}
